{
  "functions": {
    "api/generateResume.js": {
      "runtime": "edge"
    }
  }
}

{
  "version": 2,
  "builds": [
    {
      "src": "api/generateResume.js",
      "use": "@vercel/node"
    },
    {
      "src": "vite.config.js",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
